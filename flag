#!/usr/bin/python

from colorama import Fore


def cprintx(str):
    for v in str:
        if v == '-' or v == '0':
            print(Fore.WHITE + v, end='')
        else:
            print(Fore.RED + v, end='')
    print('')

print(Fore.BLACK + '-------------------------------------------------------------------' + Fore.RESET)
cprintx('XWXWXWXWXWXWXWXW00000000000000000000000000000000000WXWXWXWXWXWXWXWX')
cprintx('WXWXWXWXWXWXWXWX00000000000000000A00000000000000000XWXWXWXWXWXWXWXW')
cprintx('XWXWXWXWXWXWXWXW0000000000000000AWA0000000000000000WXWXWXWXWXWXWXWX')
cprintx('WXWXWXWXWXWXWXWX00000000000AA00AWXWA00AA00000000000XWXWXWXWXWXWXWXW')
cprintx('XWXWXWXWXWXWXWXW000000000000VWXWXWXWXWV000000000000WXWXWXWXWXWXWXWX')
cprintx('WXWXWXWXWXWXWXWX00000000AA000VWXWXWXWV000AA00000000XWXWXWXWXWXWXWXW')
cprintx('XWXWXWXWXWXWXWXW0000VWXWXWXA00VWXWXWV00AXWXWXWV0000WXWXWXWXWXWXWXWX')
cprintx('WXWXWXWXWXWXWXWX00000XWXWXWXWXWXWXWXWXWXWXWXWX00000XWXWXWXWXWXWXWXW')
cprintx('XWXWXWXWXWXWXWXW000AXWXWXWXWXWXWXWXWXWXWXWXWXWXA000WXWXWXWXWXWXWXWX')
cprintx('WXWXWXWXWXWXWXWX000000VXWXWXWXWXWXWXWXWXWXWXV000000XWXWXWXWXWXWXWXW')
cprintx('XWXWXWXWXWXWXWXW000000000VXWXWXWXWXWXWXWXV000000000WXWXWXWXWXWXWXWX')
cprintx('WXWXWXWXWXWXWXWX00000000000XWXWXWXWXWXWX00000000000XWXWXWXWXWXWXWXW')
cprintx('XWXWXWXWXWXWXWXW000000000AXWXWXWXWXWXWXWXA000000000WXWXWXWXWXWXWXWX')
cprintx('WXWXWXWXWXWXWXWX00000000000000000I00000000000000000XWXWXWXWXWXWXWXW')
cprintx('XWXWXWXWXWXWXWXW00000000000000000I00000000000000000WXWXWXWXWXWXWXWX')
cprintx('WXWXWXWXWXWXWXWX00000000000000000I00000000000000000XWXWXWXWXWXWXWXW')
cprintx('XWXWXWXWXWXWXWXW00000000000000000000000000000000000WXWXWXWXWXWXWXWX')
print(Fore.BLACK + '-------------------------------------------------------------------' + Fore.RESET)
